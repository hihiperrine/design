function fS(v,lim){lim=lim==E?1:lim;if (v==E){return "auto"}var ne=v<0?-1:1;if(v<0){v=-v}if (v<=lim){return ne*v*100+"%";}return ne*v+"px";}// [f]ix [S]ize  lim: percent threshold (default 1)     v: E->auto,-lim..lim->%, ![-lim..lim]->px
function co(e,v){e.style.background=v};// [co]lor background
function pE(e,A,p){//[p]otition [E]lement  // A:[L,T,R,B, W,H],   p:0,1,2
    e.style.position=["relative","absolute","fixed"][p==null?2:p];
    e.style.left=fS(A[0]);e.style.top=fS(A[1]);e.style.right=fS(A[2]);e.style.bottom=fS(A[3]);
